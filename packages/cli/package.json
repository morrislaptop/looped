{
  "name": "@looped-ts/cli",
  "description": "CLI utilities for the Looped Starter Kit",
  "version": "0.0.1-alpha.3",
  "author": "Craig Morris @morrislaptop",
  "bin": {
    "looped": "./bin/run"
  },
  "bugs": "https://github.com/morrislaptop/looped-ts/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^7.0.0",
    "ejs": "^2.6.2",
    "fs-extra": "^8.0.1",
    "replace-in-file": "^4.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^8",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/morrislaptop/looped-ts",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "looped",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "morrislaptop/looped-ts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
